{
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "env": {
    "browser": true,
    "node": true
  },
  "extends": [
    "plugin:vue/vue3-recommended"
  ],
  "plugins": [

  ],
  "rules": {
    "comma-dangle": "off",
    "space-before-function-paren": [
      "error",
      {
        "anonymous": "always",
        "named": "never"
      }
    ],
    "semi": "off",
    "camelcase": [
      "error",
      {
        "allow": [
          "^\\$_"
        ]
      }
    ],
    "vue/max-attributes-per-line": 0,
    "vue/singleline-html-element-content-newline": 0
  },
  "globals": {
    "ref": false,
    "reactive": false,
    "toRefs": false,
    "useHead": false,
    "useRoute": false,
    "definePageMeta": false
  }
}